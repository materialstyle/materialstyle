{
  "name": "@materialstyle/materialstyle",
  "description": "Material Style is a Material Design UI library based on Bootstrap.",
  "version": "3.0.0-alpha1",
  "config": {
    "version_short": "3.0"
  },
  "keywords": [
    "materialstyle",
    "material-style",
    "material-design",
    "css",
    "sass",
    "scss",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web",
    "bootstrap",
    "bootstrap-material-design",
    "bootstrap-theme",
    "google-material-design"
  ],
  "homepage": "https://materialstyle.github.io/",
  "author": "Neeraj Kumar Das (https://github.com/nkdas91)",
  "contributors": [
    "Material Style contributors (https://github.com/materialstyle/materialstyle/graphs/contributors)"
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch docs-serve",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.common.js --mode=development --watch",
    "docs-build": "hugo --cleanDestinationDir",
    "docs-serve": "hugo server --port 9001 --disableFastRender",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "css-lint": "npm-run-all --aggregate-output --continue-on-error --parallel css-lint-*",
    "css-lint-stylelint": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache --rd",
    "css-lint-vars": "fusv scss/ site/assets/**/scss/",
    "js-test-karma": "karma start karma.conf.js",
    "js-test-cloud": "cross-env BROWSERSTACK=true npm run js-test-karma",
    "js-test-jquery": "cross-env JQUERY=true npm run js-test-karma",
    "bundlewatch": "bundlewatch --config .bundlewatch.config.json",
    "netlify": "cross-env-shell HUGO_BASEURL=$DEPLOY_PRIME_URL npm-run-all build docs-build"
  },
  "style": "dist/css/materialstyle.min.css",
  "sass": "scss/material-style.scss",
  "main": "dist/js/materialstyle.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/materialstyle/materialstyle.git"
  },
  "bugs": {
    "url": "https://github.com/materialstyle/materialstyle/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "@popperjs/core": "^2.11.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@material/ripple": "^14.0.0",
    "@popperjs/core": "^2.11.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "autoprefixer": "^10.4.7",
    "bundlewatch": "^0.3.3",
    "clean-css-cli": "^5.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-unicorn": "^43.0.1",
    "find-unused-sass-variables": "^4.0.4",
    "globby": "^13.1.2",
    "hammer-simulator": "^0.0.1",
    "hugo-bin": "^0.89.0",
    "ip": "^1.1.8",
    "jquery": "^3.6.0",
    "karma": "^6.4.0",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-rollup-preprocessor": "^7.0.8",
    "lockfile-lint": "^4.7.6",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "rollup": "^2.76.0",
    "rollup-plugin-istanbul": "^3.0.0",
    "rtlcss": "^3.5.0",
    "sass": "^1.53.0",
    "shelljs": "^0.8.5",
    "stylelint": "^14.9.1",
    "stylelint-config-twbs-bootstrap": "^4.0.0",
    "terser": "^5.14.2",
    "vnu-jar": "^21.10.12"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src,dist}/**/*.{js,map}",
    "scss/**/*.scss"
  ],
  "hugo-bin": {
    "buildTags": "extended"
  }
}
